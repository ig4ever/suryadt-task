{
  "expo": {
    "name": "SuryaDT Task",
    "slug": "rakhmat-suryadt",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rakhmat.suryadt"
    },
    "android": {
      "package": "com.rakhmat.suryadt"
    },
    "web": {
      "favicon": "./assets/icons/app_icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/CircularStd.ttf",
            "./assets/fonts/CircularStd-Book.ttf",
            "./assets/fonts/SF-Pro-Semibold.otf"
          ]
        }
      ],
      "expo-secure-store"
    ]
  }
}
